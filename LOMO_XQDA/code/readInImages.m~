function newImages = readInImages(imgDir, imgList, imgWidth, imgHeight, imResizeMethod)
    n = length(imgList);
    READ_STD=1;
    READ_CTRL=2;
    READ_ALL=3;
    newImages=zeros(imgHeight, imgWidth, 3, n);
    for i = 1 : n
        temp = imread([imgDir, imgList(i).name]);
        
        
        switch imResizeMethod
            case READ_STD
                newImages(:,:,:,i)=imageResizeStd(images);
            case READ_CENTRAL
                newImages(:,:,:,i)=imageResizeCtrl(images);
            case READ_ALL
                newImages(:,:,:,i)=imageResizeAll(images);
            end
        newImages(:,:,:,i) = imresize(temp,[imgHeight imgWidth]);   
    end
end